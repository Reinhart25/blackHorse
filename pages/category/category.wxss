page {
  height: 100%;
}
.cates {
  height: inherit;
}
.cates .cates_contain {
  /*less 中使用calc的时候要注意*/
  height: calc(100vh - 90rpx);
  display: flex;
}
.cates .cates_contain .left_menu {
  flex: 2;
}
.cates .cates_contain .left_menu .menu_item {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cates .cates_contain .left_menu .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cates .cates_contain .right_content {
  flex: 5;
}
.cates .cates_contain .right_content .goods_group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cates .cates_contain .right_content .goods_group .goods_title .delimiter {
  color: #ccc;
}
.cates .cates_contain .right_content .goods_group .goods_title .title {
  padding-bottom: 5rpx;
}
.cates .cates_contain .right_content .goods_group .goods_list {
  display: flex;
  flex-flow: row wrap;
}
.cates .cates_contain .right_content .goods_group .goods_list navigator {
  width: 33.33%;
  text-align: center;
}
.cates .cates_contain .right_content .goods_group .goods_list navigator image {
  width: 50%;
}
